  $(document).ready(function(){
    $(".button_variant_selector").click(function(){
      $('.button_variant_selector').removeClass('selected')
      $(this).addClass("selected")
      var variant_id = $(this)[0].innerText
      console.log(variant_id)
      $('#ProductSelect-static-canvas-template-option-0').val(variant_id)
      $('#ProductSelect-static-canvas-template-option-0').change()
    })
  })

  function press_atc(){
    $('#AddToCart').click()
  }
</script>

<style>
  .call-to-action-text {
    font-size: 25px;
    line-height: 1.7em !important;
    letter-spacing: 1px !important;
    color: #000000 !important;
    text-transform: none !important;
  }

  .colored-wrap-info {
    border-radius: 8px !important;
    background-color: #f4f4f4 !important;
    padding: 20px;
  }

  #confirmation_text {
    font-size: 12px;
  }

  #loadingButtonModal.is_hidden {
    display: none;
  }

  #loadingButtonModal {
    width: 100%;
    max-width: 600px;
    min-height: auto;
    background-color: #4d4d4d;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 10px;
  }

  #loadingButtonModal {
    margin-left: 20px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: normal;
    color: #fff;
    font-family: sans-serif;
    font-size: 22px;
    font-weight: 300;
  }

  #loadingButton.is_hidden {
    display: none;
  }

  #loadingButton {
    width: 100%;
    max-width: 300px;
    min-height: auto;
    background-color: #4d4d4d;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 10px;
  }

  #loadingButtonContent {
    margin-left: 20px;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: normal;
    color: #fff;
    font-family: sans-serif;
    font-size: 22px;
    font-weight: 300;
  }

  .afterUploadContent.is_hidden {
    display: none;
  }

  #modallauncher.button_lockout {
    pointer-events: none;
    filter: grayscale(100%);
  }

  #product-bullets {
    list-style: none;
  }

  h1#productTitle {
    margin: 0 !important;
  }

  p.product-ratings {
    margin-top: -12px;
  }

  div#selector-wrapper {
    padding: 0 0 12px;
  }

  #atcwrapper {
    display: none;
  }

  #customily-options-hider {
    display: none;
  }

  .customUploadBtn.is_hidden {
    display: none;
  }

  @media screen and (max-width:768px) {
    .photoUploadWrapper {
      display: block;
    }

    div#modallauncher-wrapper {
      margin-bottom: 24px;
    }

    .change-photo-btn {
      margin: 0 0 14px 14px;
    }

  }

  .hulkapps_submit_cart {
    display: none;
  }

  .change-photo-btn {
    display: block;
    cursor: pointer;
    font-size: 16px;
    color: #ED1B67;
    margin: 14px 0 0 12px;
  }

  .product_price {
    font-size: 30px;
  }

  #imguploader {
    display: none;
  }

  .change-photo-btn.is_hidden {
    display: none;
  }

  #customily-cart-btn {
    border-radius: 8px;
    width: 100%;
    background-color: #ee1b67;
    font-weight: 300;
    font-size: 22px;
    line-height: 38px;
    padding: 4px 0;
    min-height: auto;
    max-width: 300px;
  }


  .customUploadBtn {
    width: 300px;
    cursor: pointer;
    margin: 4px;
    background-color: #e50085;
    color: #fff;
    border: 1px solid #e50085;
    border-radius: 8px;
    line-height: 40px;
    text-align: center;
    font-style: normal;
    white-space: normal;
    vertical-align: middle;
    font-size: 22px;
    font-weight: 300;
  }

  .customUploadBtn.uploaded_to {
    pointer-events: none;
    font-weight: 100;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
